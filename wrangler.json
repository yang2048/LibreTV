{
  "name": "tv",
  "compatibility_date": "2025-04-01",
  "assets": {
    "directory": "./"
  },
   "rewrites": [
    {
      "source": "/proxy/:path*",
      "destination": "/api/proxy/:path*"
    },
    {
      "source": "/s=:query",
      "destination": "/index.html"
    },
    {
      "source": "/player.html",
      "destination": "/player.html"
    },
    {
      "source": "/player.html/:path*",
      "destination": "/player.html"
    },
    {
      "source": "/:path*",
      "destination": "/:path*"
    }
  ]
}
